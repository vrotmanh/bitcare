  {
    "name": "Healthy Shopping",
    "version": "1.0",
    "description": "Healthy food shopping recommendation chrome extension",
    "permissions": ["activeTab", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
        {
          "matches": ["https://*.primenow.amazon.com/*"],
          "js": ["contentScript.js"]
        }
      ],
    "icons": { 
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
       "128": "icons/icon128.png" 
    },
    "manifest_version": 2
  }